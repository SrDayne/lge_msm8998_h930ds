/dts-v1/;

#include "../../../qcom/msm8998-v2.1.dtsi"
#include "msm8998-joan_global_ldu.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8998 v2.1 MTP";
	compatible = "qcom,msm8998-mtp", "qcom,msm8998", "qcom,mtp";
	qcom,board-id = <0x308 0>;
};

&pmi8998_charger {
	qcom,fcc-max-ua = <1600000>;
	lge,gpio-batt_temp-sel = <&tlmm 126 0>;
};

&pmi8998_fg {
	lge,batt_id = <200000>;
};

&i2c_1 { /* BLSP1 QUP1 */
	idtp9223-charger@61 {               /* Slave driver and slave Address */
		idt,configure-support-vout = <0>;
	};
};

&pm8998_vadc {
	lge,gpio-sbu-sel = <&tlmm 114 0>;
	chan@83 {
		label = "vph_pwr";
		reg = <0x83>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <1>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
    };
};

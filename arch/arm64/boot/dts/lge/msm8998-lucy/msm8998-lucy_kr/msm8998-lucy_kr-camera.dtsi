/* Flash setting*/

&pmi8998_flash0 {
	qcom,current = <800>;
	qcom,duration = <500>;
};

&pmi8998_flash1 {
	qcom,current = <800>;
	qcom,duration = <500>;
};

&pmi8998_torch0 {
	qcom,current = <100>;
};

&pmi8998_torch1 {
	qcom,current = <100>;
};

&pmi8998_switch0 {
	qcom,duration = <500>;
};

&pmi8998_switch1 {
	qcom,duration = <500>;
};

/* PM8998 GPIO Settings */
&pm8998_gpios {
	gpio@c600 { /* GPIO 7 */
		qcom,mode = <1>;		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;
		status = "ok";
	};
	gpio@c700 { /* GPIO 8 */
		qcom,mode = <1>;		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;
		status = "ok";
	};
	gpio@c800 { /* GPIO 9 */
		qcom,mode = <1>;		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;
		status = "ok";
	};
	gpio@cb00 { /* GPIO 12 */
		qcom,mode = <1>;		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <0>;		/* VIN1 GPIO_LV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;
		status = "ok";
	};
	gpio@d300 { /* GPIO 20 */
		qcom,mode = <1>;		/* Output */
		qcom,pull = <5>;		/* No Pull */
		qcom,vin-sel = <1>;		/* VIN1 GPIO_MV */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		qcom,out-strength = <1>;
		status = "ok";
	};
};

&soc {
	led_flash0: qcom,camera-flash {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		qcom,flash-type = <1>;
		qcom,flash-source = <&pmi8998_flash0 &pmi8998_flash1>;
		qcom,torch-source = <&pmi8998_torch0 &pmi8998_torch1>;
		qcom,switch-source = <&pmi8998_switch0 &pmi8998_switch1>;
		status = "ok";
	};
};

/* PIN settings */
&cam_sensor_rear_active{
	/* CAM RESET, OIS RESET, OIS MVDD, CAM VDIG, CAM AVDD */
	mux {
		pins = "gpio30", "gpio23";
	};

	config {
		pins = "gpio30", "gpio23";
		output-low;
	};
};

&cam_sensor_rear_suspend{
	/* CAM RESET, OIS RESET, OIS MVDD, CAM VDIG, CAM AVDD */
	mux {
		pins = "gpio30", "gpio23";
		function = "gpio";
	};

	config {
		pins = "gpio30", "gpio23";
		output-low;
	};
};

&cam_sensor_rear2_active {
	/* RESET, VDIG */
	mux {
		pins = "gpio28", "gpio21";
	};

	config {
		pins = "gpio28", "gpio21";
		output-low;
	};
};

&cam_sensor_rear2_suspend {
	/* RESET, VDIG */
	mux {
		pins = "gpio28", "gpio21";
		function = "gpio";
	};

	config {
		pins = "gpio28", "gpio21";
		output-low;
	};
};

&cam_sensor_front_active {
	/* RESET, CAM_VDIG */
	mux {
		pins = "gpio9";
	};

	config {
		pins = "gpio9";
		output-low;
	};
};

&cam_sensor_front_suspend {
	/* RESET, CAM_VDIG */
	mux {
		pins = "gpio9";
		function = "gpio";
	};

	config {
		pins = "gpio9";
		output-low;
	};
};

